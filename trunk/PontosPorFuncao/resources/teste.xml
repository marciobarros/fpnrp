<?xml version="1.0" encoding="UTF-8"?>
<system>
	<data-model>
		<ilf name="Empregado">
			<subset name="Empregado">
				<field name="nome" />
				<field name="matricula" />
				<field name="endereco" />
				<field name="bairro" />
				<field name="dataAdmissao" />
				<field name="dataRescisao" />
				<field name="cpf" />
				<field name="tipoContrato" />
				<field name="nomeMae" />
				<field name="nomePai" />
				<field name="dataNascimento" />
				<field name="ctps" />
				<field name="ctpsOrgaoExpeditor" />
				<field name="rg" />
				<field name="rgOrgaoExpeditor" />
				<field name="idEscolatidade" ref="Escolaridade" />
				<field name="cep" />
				<field name="idCidade" ref="Cidade" />
			</subset>
			<subset name="HistoricoFuncional">
				<field name="idEmpregado" ref="Empregado" />
				<field name="idCargo" ref="Cargo" />
				<field name="dataInicio" />
				<field name="dataTermino" />
			</subset>
			<subset name="HoraExtra">
				<field name="idEmpregado" ref="Empregado" />
				<field name="idGerente" ref="Gerente" dataModelElement="Empregado"/>
				<field name="dataHoraExtra" />
				<field name="horaInicio" />
				<field name="horaTermino" />
			</subset>
			<subset name="Afastamento">
				<field name="idEmpregado" ref="Empregado" />
				<field name="idMotivoAfastamento" ref="MotivoAfastamento" />
				<field name="dataInicio" />
				<field name="dataRetorno" />
			</subset>
			<subset name="Gerente">
				<field name="idEmpregado" ref="Empregado" />
				<field name="idCentroDeCusto" ref="CentroDeCusto" dataModelElement="Estabelecimento"/>
				<field name="dataInicio" />
				<field name="dataTermino" />
			</subset>
			<subset name="HistoricoLotacao">
				<field name="idEmpregado" ref="Empregado" />
				<field name="idCentroDeCusto" ref="CentroDeCusto" dataModelElement="Estabelecimento" />
				<field name="idMotivoTransferencia" ref="MotivoTransferencia" />
				<field name="dataInicio" />
				<field name="dataTermino" />
			</subset>
			<subset name="Falta">
				<field name="idEmpregado" ref="Empregado" />
				<field name="idMotivoFalta" ref="MotivoFalta" />
				<field name="idGerente" ref="Gerente" dataModelElement="Empregado" description="Gerente que abonou a falta!"/>
				<field name="dataFalta" />
				<field name="dataTermino" />
			</subset>
			<subset name="ContraCheque">
				<field name="idEmpregado" ref="Empregado" />
				<field name="anoMes" />
				<field name="tipoPagamento" />
			</subset>
			<subset name="ContraChequeVerba">
				<field name="idContraCheque" ref="ContraCheque" dataModelElement="Empregado" />
				<field name="idVerba" ref="Verba" />
				<field name="valor" />
			</subset>
		</ilf>
		<ilf name="MotivoAfastamento">
			<subset name="MotivoAfastamento">
				<field name="descricao"/>
			</subset>
		</ilf>
		<ilf name="Estabelecimento">
			<subset name="Estabelecimento">
				<field name="idCidade" ref="Cidade" />
				<field name="descricao"/>
				<field name="endereco"/>
				<field name="cep"/>
				<field name="bairro"/>
			</subset>
			<subset name="CentroDeCusto">
				<field name="idEstabelecimento" ref="Estabelecimento" />
				<field name="descricao"/>
				<field name="sigla"/>
			</subset>			
		</ilf>
		<ilf name="MotivoTransferencia">
			<subset name="MotivoTransferencia">
				<field name="descricao"/>
			</subset>
		</ilf>
		<ilf name="MotivoFalta">
			<subset name="MotivoFalta">
				<field name="descricao"/>
			</subset>
		</ilf>
		<ilf name="Verba">
			<subset name="Verba">
				<field name="descricao"/>
			</subset>
		</ilf>
		<ilf name="Cargo">
			<subset name="Cargo">
				<field name="descricao"/>
			</subset>
			<subset name="SalarioCargo">				
				<field name="idCargo" ref="Cargo"/>
				<field name="valor"/>
				<field name="anoMesInicio"/>
				<field name="anoMesTermino"/>
			</subset>
		</ilf>
		<eif name="Cidade">
			<subset name="Cidade">
				<field name="nome" />
				<field name="estado" />
			</subset>
		</eif>
		<eif name="Escolaridade">
			<subset name="Escolaridade">
				<field name="descricao" />
			</subset>
		</eif>
	</data-model>

	<transaction-model>
		<transaction name="IncluirNovoEmpregado" errorMsg="true" type="EI">
			<ftr-list>
				<ftr name="Empregado" subset="Empregado" useAllFields="true" />
				<ftr name="Cidade" subset="Cidade">
					<field name="nome"/>
				</ftr>
				<ftr name="Escolaridade">
					<field name="descricao"/>
				</ftr>
			</ftr-list>			
		</transaction>
		<transaction name="AlterarEmpregado" errorMsg="true" type="EI">
			<ftr-list>
				<ftr name="Empregado" subset="Empregado" useAllFields="true" />
				<ftr name="Cidade" subset="Cidade">
					<field name="nome"/>
				</ftr>
				<ftr name="Escolaridade"  subset="Escolaridade">
					<field name="descricao"/>
				</ftr>
			</ftr-list>
			<dependencies>
					<dependency ref="IncluirNovoEmpregado"/>
					<dependency ref="ConsultarEmpregado"/>
			</dependencies>			
		</transaction>
		<transaction name="RemoverEmpregado" errorMsg="true" type="EI">
			<ftr-list>
				<ftr name="Empregado" subset="Empregado">
					<field name="matricula"/>
					<field name="nome"/>
				</ftr>
			</ftr-list>
			<dependencies>
					<dependency ref="IncluirNovoEmpregado"/>
					<dependency ref="ConsultarEmpregado"/>
			</dependencies>			
		</transaction>
		<transaction name="ConsultarEmpregado" errorMsg="true" type="EQ">
			<ftr-list>
				<ftr name="Empregado" subset="Empregado">
					<field name="matricula"/>
					<field name="nome"/>
					<field name="dataAdmissao"/>
					<field name="dataRescisao"/>
				</ftr>
			</ftr-list>
			<dependencies>
					<dependency ref="IncluirNovoEmpregado"/>
			</dependencies>			
		</transaction>
		<transaction name="IncluirAfastamento" errorMsg="true" type="EI">
			<ftr-list>
				<ftr name="Afastamento" subset="Afastamento" dataModelElement="Empregado">
					<field name="dataInicio"/>
				</ftr>
				<ftr name="MotivoAfastamento">
				 	<field name="descricao"/>
				</ftr>
				<ftr name="Empregado">
				 	<field name="matricula"/>
				 	<field name="nome"/>
				</ftr>
			</ftr-list>
			<dependencies>
					<dependency ref="IncluirNovoEmpregado"/>
					<dependency ref="IncluirMotivoDeAfastamento" canBeWeak="true"/>
			</dependencies>			
		</transaction>
		<transaction name="AlterarAfastamento" errorMsg="true" type="EI">
			<ftr-list>
				<ftr name="Afastamento"  subset="Afastamento" dataModelElement="Empregado" useAllFields="true"/>
				<ftr name="MotivoAfastamento">
				 	<field name="descricao"/>
				</ftr>
				<ftr name="Empregado">
				 	<field name="matricula"/>
				 	<field name="nome"/>
				</ftr>				
			</ftr-list>
			<dependencies>
					<dependency ref="IncluirAfastamento"/>
					<dependency ref="ConsultarAfastamentos"/>
			</dependencies>			
		</transaction>
		<transaction name="CancelarAfastamento" errorMsg="true" type="EI">
			<ftr-list>
				<ftr name="Afastamento" subset="Afastamento" dataModelElement="Empregado">
					<field name="dataInicio"/>
				</ftr>
				<ftr name="Empregado">
				 	<field name="matricula"/>
				 	<field name="nome"/>
				</ftr>
			</ftr-list>
			<dependencies>
					<dependency ref="IncluirAfastamento"/>
					<dependency ref="ConsultarAfastamentos"/>
			</dependencies>			
		</transaction>
		<transaction name="ConsultarAfastamentos" errorMsg="true" type="EQ">
			<ftr-list>
				<ftr name="Afastamento" subset="Afastamento" dataModelElement="Empregado" useAllFields="true"/>
				<ftr name="MotivoAfastamento">
				 	<field name="descricao"/>
				</ftr>
				<ftr name="Empregado">
