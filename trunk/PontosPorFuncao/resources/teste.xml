			</ftr-list>
			<dependencies>
					<dependency ref="IncluirNovoEmpregado"/>
					<dependency ref="DesignarGerencia"/>
			</dependencies>			
		</transaction>
		<transaction name="AlterarHoraExtra" errorMsg="true" type="EI">
			<ftr-list>
