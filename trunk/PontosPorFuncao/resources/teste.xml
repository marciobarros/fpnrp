		<transaction name="RemoverLotacao" errorMsg="true" type="EI">
			<ftr-list>
				<ftr name="HistoricoLotacao" subset="HistoricoLotacao" dataModelElement="Empregado">
					<field name="dataInicio"/>
				</ftr>>
				<ftr name="Empregado">
				 	<field name="matricula"/>
				 	<field name="nome"/>
				</ftr>
			</ftr-list>
			<dependencies>
