				<ftr name="Gerente" subset="Gerente" dataModelElement="Empregado">
				</ftr>
			</ftr-list>
			<dependencies>
					<dependency ref="IncluirFalta"/>
					<dependency ref="ConsultarFaltas"/>
					<dependency ref="DesignarGerencia"/>
			</dependencies>			
		</transaction>
		
		
		<transaction name="RemoverFalta" errorMsg="true" type="EI">
			<ftr-list>
				<ftr name="Falta" dataModelElement="Empregado" subset="Falta">
