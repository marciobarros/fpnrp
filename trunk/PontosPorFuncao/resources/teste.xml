			</dependencies>			
		</transaction>
		<transaction name="ConsultarLotacoes" errorMsg="true" type="EQ">
			<ftr-list>
				<ftr name="HistoricoLotacao" subset="HistoricoLotacao" dataModelElement="Empregado">
					<field name="dataInicio"/>
				</ftr>>
				<ftr name="Empregado">
				 	<field name="matricula"/>
				 	<field name="nome"/>
				</ftr>
			</ftr-list>
			<dependencies>
					<dependency ref="IncluirLotacao"/>
			</dependencies>			
		</transaction>

		<transaction name="IncluirFalta" errorMsg="true" type="EI">
			<ftr-list>
				<ftr name="Falta" subset="Falta" dataModelElement="Empregado" useAllFields="true"/>
